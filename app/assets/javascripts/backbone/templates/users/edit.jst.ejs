<h1>Edit user</h1>

<form id="edit-user" name="user">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value=<%= name %> >
  </div>

  <div class="field">
    <label for="last_login_at"> last_login_at:</label>
    <input type="text" name="last_login_at" id="last_login_at" value=<%= last_login_at %> >
  </div>

  <div class="actions">
    <input type="submit" value="Update User" />
  </div>

</form>

